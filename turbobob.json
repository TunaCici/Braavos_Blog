{
	"for_description_of_this_file_see": "https://github.com/function61/turbobob",
	"version_major": 1,
	"project_name": "joonas.fi-blog",
	"builders": [
		{
			"name": "default",
			"dockerfile_path": "build-default.Dockerfile",
			"mount_destination": "/project",
			"dev_command": ["bash"],
			"dev_ports": ["8084:80"],
			"dev_pro_tips": ["For preview run $ bin/preview.sh"],
			"pass_envs": [],
			"contextless_build": true
		}
	],
	"docker_images": [
		{
			"image": "joonas/joonas.fi-blog",
			"dockerfile_path": "Dockerfile",
			"auth_type": "creds_from_env"
		}
	]
}
